{
	"_version": "1.12.0",

	"sap.app": {
		"id": "com.gavdilabs.StepX",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/odata/",
				"type": "OData",
				"settings": {
					"localUri": "localService/metadata.xml",
					"odataVersion": "4.0"
				}
			}
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {

		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "com.gavdilabs.StepX.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},

		"dependencies": {
			"minUI5Version": "1.108.10",
			"libs": {
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ui.unified": {},
				"sap.m": {}
			}
		},

		"handleValidation": true,

		"contentDensities": {
			"compact": true,
			"cozy": true
		},

		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.gavdilabs.StepX.i18n.i18n"
				}
			},
			"": {
				"dataSource": "test-service",
				"preload": true,
				"type": "sap.ui.model.odata.v4.ODataModel",
				"settings": {
					"synchronizationMode": "None",
					"operationMode": "Server"
				}
			}
		},

		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.gavdilabs.StepX.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "main",
					"target": "main"
				},
				{
					"pattern": "Register",
					"name": "register",
					"target": "register"
				}
			],
			"targets": {
				"main": {
					"viewId": "main",
					"viewName": "Main"
				},
				"register": {
					"viewId": "register",
					"viewName": "Registration"
				}
			}
		}
	}
}